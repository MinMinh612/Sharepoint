{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "last-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/last/LastWebPart.js",
          "manifest": "./src/webparts/last/LastWebPart.manifest.json"
        }
      ]
    },
    "suggest-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/suggest/SuggestWebPart.js",
          "manifest": "./src/webparts/suggest/SuggestWebPart.manifest.json"
        }
      ]
    },
    "approve-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/approve/ApproveWebPart.js",
          "manifest": "./src/webparts/approve/ApproveWebPart.manifest.json"
        }
      ]
    },
    "home-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/home/HomeWebPart.js",
          "manifest": "./src/webparts/home/HomeWebPart.manifest.json"
        }
      ]
    },
    "permission-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/permission/PermissionWebPart.js",
          "manifest": "./src/webparts/permission/PermissionWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "react": "React",
    "react-dom": "ReactDOM"
  },
  "localizedResources": {
    "LastWebPartStrings": "lib/webparts/last/loc/{locale}.js",
    "SuggestWebPartStrings": "lib/webparts/suggest/loc/{locale}.js",
    "ApproveWebPartStrings": "lib/webparts/approve/loc/{locale}.js",
    "HomeWebPartStrings": "lib/webparts/home/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PermissionWebPartStrings": "lib/webparts/permission/loc/{locale}.js"
  }
}
